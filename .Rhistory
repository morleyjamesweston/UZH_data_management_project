plus <- function(x,y){
return(x+y)
}
times <- function(x,y){
return(x*y)
}
plus_or_times <- function(x,y, mode = "plus"){
if (mode == "plus"){
return( plus(x,y) )
}
else if (mode == "times"){
return( times(x,y) )
}
}
